{
  "name": "backend",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "database:install": "npx prismix && prisma generate",
    "local:start": "env-cmd -f .env.local vite",
    "dev:start": "env-cmd -f .env.development vite",
    "dev:build": "tsc && vite build --mode development",
    "uat:start": "env-cmd -f .env.staging vite",
    "uat:build": "tsc && vite build --mode staging",
    "product:start": "env-cmd -f .env.production vite",
    "product:build": "tsc && vite build",
    "pack": "command/pack.bat",
    "serve": "node dist/server.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/express-prometheus-middleware": "^1.2.3",
    "@types/html-pdf-node": "^1.0.0",
    "@types/joi": "^17.2.3",
    "@types/lodash": "^4.14.195",
    "@types/multer": "^1.4.7",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^3.0.0",
    "prisma": "^5.7.1",
    "prismix": "^1.0.22",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vite-plugin-node": "^3.0.2"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "emitter": "^0.0.5",
    "express": "^4.18.2",
    "express-prometheus-middleware": "^1.2.0",
    "express-winston": "^4.2.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.1",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "prom-client": "^15.0.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  }
}
